{
  "name": "@firekit/api",
  "version": "1.0.2-alpha.5",
  "description": "Opinionated library of Firestore components, models, clients, etc.",
  "author": "Michael Martin <martinmsm@gmail.com>",
  "homepage": "https://github.com/michael-martin-al/firekit",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-martin-al/firekit.git"
  },
  "scripts": {
    "test": "npx jest",
    "build": "npm run compile",
    "compile": "BABEL_ENV=build ../../node_modules/.bin/babel --root-mode upward src --out-dir lib --source-maps --extensions .js --delete-dir-on-start --no-comments",
    "prepare": "npm run compile",
    "watch": "BABEL_ENV=build ../../node_modules/.bin/babel --root-mode upward src --out-dir lib --source-maps --extensions .js --delete-dir-on-start --no-comments --watch"
  },
  "devDependencies": {
    "firebase": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-query": "^1.2.9"
  },
  "peerDependencies": {
    "firebase": "^7.14.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-query": "^1.2.9"
  }
}
